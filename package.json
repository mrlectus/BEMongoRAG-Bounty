{
  "name": "bemongorag-bounty",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register test/**/*.ts",
    "start": "pnpm run build && fastify start -l info dist/app.js",
    "build": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P --port 4000 dist/app.js",
    "postinstall": "npm run build",
    "tsc": "./node_modules/typescript/bin/tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/sensible": "^5.0.0",
    "@langchain/core": "^0.1.62",
    "@langchain/mongodb": "^0.0.1",
    "@langchain/openai": "^0.0.28",
    "@sinclair/typebox": "^0.32.29",
    "ai": "^3.1.5",
    "env-schema": "^5.2.1",
    "fastify": "^4.26.1",
    "fastify-cli": "^6.1.1",
    "fastify-plugin": "^4.0.0",
    "fastify-type-provider-zod": "^1.1.9",
    "langchain": "^0.1.36",
    "pdf-parse": "^1.1.1",
    "zod": "^3.23.6",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "c8": "^9.0.0",
    "concurrently": "^8.2.2",
    "fastify-tsconfig": "^2.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  }
}
